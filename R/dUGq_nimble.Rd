\name{dUGq_nimble}
\alias{dUGq_nimble}
\title{UGq density for NIMBLE}
\description{Density function for the Unified UGq distribution, implemented as a NIMBLE custom distribution.}
\usage{
dUGq_nimble(x, mu, sigma, tau, log = FALSE)
}
\arguments{
\item{x}{Observed value in (0,1).}
\item{mu}{Mean parameter.}
\item{sigma}{Dispersion parameter.}
\item{tau}{Quantile level.}
\item{log}{Logical; return log-density.}
}
